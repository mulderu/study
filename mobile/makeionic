DATE=`date +%Y%m%d`
TIME=`date +%H%M%S`

APP_ID=`date +%Y%m%d_%H%M%S`

if [ "$#" -eq 1 ]; then
    APP_ID="$1"
fi

# blank, tabs, sidemenu
echo "make ... ionic start app$APP_ID sidemenu -a MyApp$APP_ID -i com.turbinesoft.myapp$APP_ID"
